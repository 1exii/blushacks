{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/product",
        "dest": "/Product.jsx"
      },
      {
        "src": "/product/(.*)",
        "dest": "/Product.jsx"
      },
      {
        "src": "/",
        "dest": "/Home.jsx"
      }
    ]
  }